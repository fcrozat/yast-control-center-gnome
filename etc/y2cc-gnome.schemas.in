<gconfschemafile>
	<schemalist>
		<schema>
			<key>/schemas/desktop/gnome/applications/y2cc-gnome/cc_actions_list</key>
			<applyto>/desktop/gnome/applications/y2cc-gnome/cc_actions_list</applyto>
			<owner>y2cc-gnome</owner>
			<type>list</type>
			<list_type>string</list_type>
			<locale name="C">
				<default>[Add a User;YaST2/users.desktop,Graphics Setup;YaST2/x11.desktop]</default>
				<short>Task names and associated .desktop files</short>
				<long>The task name to be displayed in the y2cc-gnome (thus needing to be translated) followed by a ";" separator then the filename of an associated .desktop file to launch for that task.</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/desktop/gnome/applications/y2cc-gnome/cc_exit_shell_on_static_action</key>
			<applyto>/desktop/gnome/applications/y2cc-gnome/cc_exit_shell_on_static_action</applyto>
			<owner>y2cc-gnome</owner>
			<type>bool</type>
			<default>false</default>
			<locale name="C">
				<short>Close the y2cc-gnome when a task is activated</short>
				<long>if true, the y2cc-gnome will close when a "Common Task" is activated</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/desktop/gnome/applications/y2cc-gnome/cc_exit_shell_on_action_start</key>
			<applyto>/desktop/gnome/applications/y2cc-gnome/cc_exit_shell_on_action_start</applyto>
			<owner>y2cc-gnome</owner>
			<type>bool</type>
			<default>false</default>
			<locale name="C">
				<short>Exit shell on start action performed</short>
				<long>Indicates whether to close the shell when a start action is performed</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/desktop/gnome/applications/y2cc-gnome/cc_exit_shell_on_action_help</key>
			<applyto>/desktop/gnome/applications/y2cc-gnome/cc_exit_shell_on_action_help</applyto>
			<owner>y2cc-gnome</owner>
			<type>bool</type>
			<default>false</default>
			<locale name="C">
				<short>Exit shell on help action performed</short>
				<long>Indicates whether to close the shell when a help action is performed</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/desktop/gnome/applications/y2cc-gnome/cc_exit_shell_on_action_add_remove</key>
			<applyto>/desktop/gnome/applications/y2cc-gnome/cc_exit_shell_on_action_add_remove</applyto>
			<owner>y2cc-gnome</owner>
			<type>bool</type>
			<default>false</default>
			<locale name="C">
				<short>Exit shell on add or remove action performed</short>
				<long>Indicates whether to close the shell when an add or remove action is performed</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/desktop/gnome/applications/y2cc-gnome/cc_exit_shell_on_action_upgrade_uninstall</key>
			<applyto>/desktop/gnome/applications/y2cc-gnome/cc_exit_shell_on_action_upgrade_uninstall</applyto>
			<owner>y2cc-gnome</owner>
			<type>bool</type>
			<default>false</default>
			<locale name="C">
				<short>Exit shell on upgrade or uninstall action performed</short>
				<long>Indicates whether to close the shell when an upgrade or uninstall action is performed</long>
			</locale>
		</schema>
	</schemalist>
</gconfschemafile>
